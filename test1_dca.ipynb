    "class DCA(Strategy):\n",
    "\n",
    "    amount_to_invest = 10\n",
    "\n",
    "    def init(self):\n",
    "\n",
    "        self.day_of_week = self.I(\n",
    "                            lambda x: x,\n",
    "                            self.data.Close.s.index.dayofweek,\n",
    "                            plot = False,\n",
    "                            )\n",
    "\n",
    "    def next(self):\n",
    "\n",
    "        if self.day_of_week[-1] == 1:\n",
    "            self.buy( size = math.floor(self.amount_to_invest / self.data.Close[-1]))\n",
    "            
   ]
